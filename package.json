{
  "name": "doc-chomp",
  "version": "1.1.0",
  "description": "Doc Chomp chomps on your document literals",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib --source-maps inline --ignore \"**/*.spec.js\"",
    "test": "jest",
    "lint": "eslint src",
    "lint-and-fixup": "eslint --fix src"
  },
  "keywords": [
    "documents",
    "es6",
    "es2015",
    "literals",
    "template literals",
    "trimming",
    "heredoc"
  ],
  "author": "Jessica Stokes <hello@jessicastokes.net>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.5.0",
    "eslint": "5.15.2",
    "jest": "24.5.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "collectCoverage": true
  },
  "dependencies": {
    "literal-toast": "^1.0.0"
  }
}
