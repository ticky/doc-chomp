{
  "name": "doc-chomp",
  "version": "1.1.0",
  "description": "Doc Chomp chomps on your document literals",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib --source-maps inline --ignore \"**/*.spec.js\"",
    "test": "jest",
    "lint": "eslint src",
    "lint-and-fixup": "eslint --fix src"
  },
  "keywords": [
    "documents",
    "es6",
    "es2015",
    "literals",
    "template literals",
    "trimming",
    "heredoc"
  ],
  "author": "Jessica Stokes <hello@jessicastokes.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.16.0",
    "eslint": "^3.8.1",
    "jest": "^18.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "collectCoverage": true
  },
  "dependencies": {
    "literal-toast": "^1.0.0"
  }
}
